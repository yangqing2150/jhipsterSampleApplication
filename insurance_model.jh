entity InsInsureInfo(INS_INSURE_INFO){//非车投保信息业务数据表
				id	Long	required, //ID	
				version	Long required,//版本号
				dataId	String required,//投保数据ID	
				houseId	Long,//房屋信息ID	
				invoiceId	Long,//配送信息ID
				orderId String ,//订单ID
				personId	Long,//投保人保单相关信息ID
				policyId	Long required,//保单ID	
				premiumPrice	BigDecimal,//保单价格	
				propMapStr	String,//json串数据 CLOB
				insuredId	Long,//关系人信息ID	
				dateCreated ZonedDateTime,// timestamp 创建时间
				creator String,// varchar2(100 char) 创建人
				lastUpdate ZonedDateTime,// timestamp 更新时间
				modifier String// varchar2(100 char) 更新人
}

entity InsPPersonInfo(INS_P_PERSON_INFO){ //非车被保险人信息业务数据表
				id	Long required, //ID	
				version	Long required,//版本号	
				address	String,//地址	
				address2	String,//地址2	
				area	String,//所在地区	
				birthday	ZonedDateTime,//出生年月日	
				businessSort	String,//业务分类	
				businessType	String,//意外险行业代码	
				city	String,//城市	
				copies	Long,//购买分数	
				email	String,//电子邮箱	
				idNum	String,//证件号码	
				idType	String,//证件类型	
				job	String,//被保险人职业	
				jobType	String,//被保险人职业类别	
				name	String,//名称	
				occupation	String,//职业	
				occupationCode	String,//意外险职业代码	
				phoneNum1	String,//手机	
				phoneNum2	String,//固话	
				phoneNum3	String,//紧急联系人电话	
				postCode	String,//邮编	
				propMapStr	String,//json串数据	CLOB
				province	String,//省份	
				relationShip	String,//与投保人关系	
				sex	String,//性别	
				type	String,//类型，投保人，初保险人	
				dateCreated ZonedDateTime,// timestamp 创建时间
				creator String,// varchar2(100 char) 创建人
				lastUpdate ZonedDateTime,// timestamp 更新时间
				modifier String// varchar2(100 char) 更新人
}

