relationship OneToOne {
    InsPersonInfo{insInsureInfo(personId)} to InsInsureInfo{insPersonInfo}
    InsPolicyInfo{insInsureInfo(policyId)} to InsInsureInfo{insPolicyInfo}
    
}
relationship OneToMany {
		InsInsureInfo{insItemkindInfo} to InsItemkindInfo{insInsureInfo(insuredId)}
		InsInsureInfo{insPPersonInfo} to InsPPersonInfo{insInsureInfo(dataId)}
}


// Set pagination options
paginate InsInsureInfo,InsPPersonInfo,InsInvoiceInfo,InsPolicyInfo,InsPersonInfo,InsItemkindInf,InsHouseInfo,InsureData with pagination

dto * with mapstruct

// Set service options to all except few
service all with serviceImpl
// Set an angular suffix
angularSuffix * with hbinsInsure