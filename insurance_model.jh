entity InsureData(INSURE_DATA){ //非车险投保流程数据表
				id	String required,//投保数据ID	
				orderId	String,//订单ID	
				version	Long required,//版本号	
				insureStr	String,//投保信息	CLOB
				policyBackStr	String,//保单返回信息	CLOB
				policyNo	String,//保单号	
				policyStr	String,//保单信息	CLOB
				productCode	String required,//产品代码	
				proposalBackStr	String,//投保单返回信息	CLOB
				proposalNo	String,//投保单号	
				proposalStr	String,//投保单信息	CLOB
				quoteStr	String,//报价信息	CLOB
				riskCode	String,//险种代码	
				step	Long,//当前步骤	
				userId	String,//用户ID	
				dataSource	String,//业务数据来源	
				dateCreated ZonedDateTime,// timestamp 创建时间
				creator String,// varchar2(100 char) 创建人
				lastUpdate ZonedDateTime,// timestamp 更新时间
				modifier String// varchar2(100 char) 更新人
}

